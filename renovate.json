{
  "automerge": false,
  "dependencyDashboard": true,
  "enabledManagers": ["yarn"],
  "labels": ["dependency-update"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"]
    },
    {
      "addLabels": ["major"],
      "matchUpdateTypes": ["major"]
    }
  ],
  "prConcurrentLimit": 5,
  "prHourlyLimit": 10,
  "rangeStrategy": "bump",
  "rebaseWhen": "behind-base-branch"
}
